<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.deri.wsml.eclipse.discovery"
   name="Discovery Plug-in"
   version="0.2"
   provider-name="STI Innsbruck"
   class="org.deri.wsml.eclipse.discovery.Activator">

   <runtime>
      <library name="discovery.jar">
         <export name="*"/>
      </library>
      <library name="lib/axiom-api-1.2.5.jar">
         <export
               name="*">
         </export>
      </library>
      <library name="lib/axiom-impl-1.2.5.jar">
         <export
               name="*">
         </export>
      </library>
      <library name="lib/axis2-adb-1.3.jar">
         <export
               name="*">
         </export>
      </library>
      <library name="lib/axis2-kernel-1.3.jar">
         <export
               name="*">
         </export>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="net.sourceforge.wsmo4j" export="false"/>
      <import plugin="org.deri.wsml.eclipse.reasoner"/>
      <import plugin="org.openoasis.see.integration"/>
      <import plugin="org.deri.wsmx.integration.eclipse" version="0.3.0"/>
   </requires>

</plugin>
